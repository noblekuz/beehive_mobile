      "bundleIdentifier": "com.basket.beeHive",
      "buildNumber": "4.0"
    },
    "android": {
      "package": "com.basket.beeHive",
      "versionCode": 4,
      "permissions" : []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
